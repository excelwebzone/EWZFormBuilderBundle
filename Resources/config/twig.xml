<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="ewz_form_builder.twig.form_builder_extension.class">EWZ\Bundle\FormBuilderBundle\Twig\Extension\FormBuilderExtension</parameter>
    </parameters>

    <services>
        <service id="ewz_form_builder.twig.form_builder_extension" class="%ewz_form_builder.twig.form_builder_extension.class%">
            <tag name="twig.extension" />
        </service>
    </services>
</container>
